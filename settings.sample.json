{
  "server": {
    "dnsName": "acme.example.com",
    "ports": {
      "https": 443,
      "http": 80
    }
  },
  "keyStore": {
    "type": "pkcs12",
    "password": "test",
    "location": "./serverdata/test.p12"
  },
  "database": {
    "engine": "h2",
    "host": "",
    "user": "root",
    "password": "changeme",
    "name": "./serverdata/acme"
  },
  "emailSmtp": {
    "enabled": false,
    "port": 587,
    "encryption": "starttls",
    "host": "mail.example.com",
    "username": "acme-noreply@example.com",
    "password": "insecurepassword"
  },
  "rootCA": {
    "metadata": {
      "commonName": "ACMEServer Root CA",
      "organisation": null,
      "organisationalUnit": null,
      "countryCode": null
    },
    "expiration": {
      "days": 3,
      "months": 6,
      "years": 10
    },
    "algorithm": {
      "type": "rsa",
      "keySize": 4096
    }
  },
  "provisioner": [
    {
      "name": "testing",
      "useThisProvisionerForAcmeApi": false,
      "meta": {
        "website": "https://example.com/testing-ca",
        "tos": " https://example.com/terms-of-service-testing.html"
      },
      "intermediate": {
        "metadata": {
          "commonName": "Testing Intermediate",
          "organisation": null,
          "organisationalUnit": null,
          "countryCode": null
        },
        "expiration": {
          "days": 1,
          "months": 1,
          "years": 1
        },
        "algorithm": {
          "type": "ecdsa",
          "curveName": "secp256k1"
        }
      },
      "issuedCertificateExpiration": {
        "days": 3,
        "months": 0,
        "years": 0
      },
      "wildcardAllowed": false,
      "domainNameRestriction": {
        "enabled": false,
        "mustEndWith": [
          ".test.example.com",
          ".test.example.org"
        ]
      }
    },
    {
      "name": "production",
      "useThisProvisionerIntermediateForAcmeApi": true,
      "meta": {
        "website": "https://example.com/production-ca",
        "tos": " https://example.com/terms-of-service-production.html"
      },
      "intermediate": {
        "metadata": {
          "commonName": "Production Intermediate",
          "organisation": null,
          "organisationalUnit": null,
          "countryCode": null
        },
        "expiration": {
          "days": 1,
          "months": 1,
          "years": 1
        },
        "algorithm": {
          "type": "rsa",
          "keySize": 4096
        }
      },
      "issuedCertificateExpiration": {
        "days": 0,
        "months": 1,
        "years": 0
      },
      "wildcardAllowed": true,
      "domainNameRestriction": {
        "enabled": false,
        "mustEndWith": [
          ".prod.example.com",
          ".prod.example.org"
        ]
      }
    }
  ],
  "proxy": {
    "httpChallenge": {
      "enabled": false,
      "type": "socks",
      "host": "127.0.0.1",
      "port": 9050,
      "authentication": {
        "enabled": false,
        "username": "username",
        "password": "password"
      }
    }
  }
}