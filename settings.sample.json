{
  "$schema": "schema.settings.json",
  "server": {
    "dnsName": "acme.example.com",
    "ports": {
      "https": 443,
      "http": 80
    },
    "loggingDirectory": "./serverdata/logs",
    "mozillaSslConfig": {
      "enabled": false,
      "version": "5.7",
      "configuration": "intermediate"
    },
    "sslServerConfig": {
      "allowLegacyResumption": false,
      "enableSniCheck": true
    }
  },
  "keyStore": {
    "type": "pkcs12",
    "password": "test",
    "location": "./serverdata/test.p12"
  },
  "database": {
    "jdbcUrl": "jdbc:h2:./serverdata/acme;DB_CLOSE_DELAY=-1",
    "user": "root",
    "password": ""
  },
  "rootCA": {
    "metadata": {
      "commonName": "ACMEServer Root CA",
      "organisation": null,
      "organisationalUnit": null,
      "countryCode": null
    },
    "expiration": {
      "days": 3,
      "months": 6,
      "years": 10
    },
    "algorithm": {
      "type": "rsa",
      "keySize": 4096
    }
  },
  "provisioner": [
    {
      "name": "testing",
      "meta": {
        "website": "https://example.com/testing-ca",
        "tos": " https://example.com/terms-of-service-testing.html"
      },
      "intermediate": {
        "metadata": {
          "commonName": "Testing Intermediate",
          "organisation": null,
          "organisationalUnit": null,
          "countryCode": null
        },
        "expiration": {
          "days": 1,
          "months": 1,
          "years": 1
        },
        "algorithm": {
          "type": "ecdsa",
          "curveName": "P-256"
        }
      },
      "issuedCertificateExpiration": {
        "days": 3,
        "months": 0,
        "years": 0
      },
      "wildcardAllowed": false,
      "ipAllowed": true,
      "domainNameRestriction": {
        "enabled": false,
        "mustEndWith": [
          ".test.example.com",
          ".test.example.org"
        ]
      }
    },
    {
      "name": "production",
      "meta": {
        "website": "https://example.com/production-ca",
        "tos": " https://example.com/terms-of-service-production.html"
      },
      "intermediate": {
        "metadata": {
          "commonName": "Production Intermediate",
          "organisation": null,
          "organisationalUnit": null,
          "countryCode": null
        },
        "expiration": {
          "days": 1,
          "months": 1,
          "years": 1
        },
        "algorithm": {
          "type": "rsa",
          "keySize": 4096
        }
      },
      "issuedCertificateExpiration": {
        "days": 0,
        "months": 1,
        "years": 0
      },
      "wildcardAllowed": true,
      "ipAllowed": true,
      "domainNameRestriction": {
        "enabled": false,
        "mustEndWith": [
          ".prod.example.com",
          ".prod.example.org"
        ]
      }
    }
  ],
  "network": {
    "proxy": {
      "enabled": false,
      "type": "socks",
      "host": "127.0.0.1",
      "port": 9050,
      "authentication": {
        "enabled": false,
        "username": "username",
        "password": "password"
      }
    },
    "dnsConfig": {
      "dnsServers": [
        "8.8.8.8",
        "8.8.4.4",
        "2001:4860:4860::8888",
        "2001:4860:4860::8844"
      ],
      "dohEnabled": false,
      "dohEndpoint": "https://cloudflare-dns.com/dns-query"
    }
  }
}
